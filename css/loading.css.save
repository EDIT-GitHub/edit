#loaderLayer {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background-color: #000000;
  z-index: 10000;
  text-align: center;
  transition: transform 0.3s ease-out;
  -webkit-transition: transform 0.3s ease-out;
}

#loaderLayer.remove {
  transform: translate(0,100%);
  -webkit-transform: translate(0,100%);
}

#loaderLayer .logo-anim {
  position: fixed;
  top: calc(50% + 90px);
  left: 50%;
  width: 180px;
  height: 180px;
  transform: translate(-50%,-50%);
  -webkit-transform: translate(-50%,-50%);
}

#loaderLayer .logo-anim span {
  position: absolute;
  width: 42px;
  height: 42px;
  border: 2px solid white;
}

#loaderLayer .logo-anim span svg {
  position: absolute;
  left: -2px;
  top: -2px;
}

#loaderLayer .logo-anim span.logo-e {
  top: 0;
  left: 0;
  animation: 6s logoAnim-e infinite;
  -webkit-animation: 6s logoAnim-e infinite;
}

#loaderLayer .logo-anim span.logo-d {
  top: 0;
  left: 40px;
  animation: 6s logoAnim-d infinite;
  -webkit-animation: 6s logoAnim-d infinite;
}

#loaderLayer .logo-anim span.logo-i {
  top: 0;
  left: 80px;
  animation: 6s logoAnim-i infinite;
  -webkit-animation: 6s logoAnim-i infinite;
}

#loaderLayer .logo-anim span.logo-t {
  top: 0;
  left: 120px;
  animation: 6s logoAnim-t infinite;
  -webkit-animation: 6s logoAnim-t infinite;
}

#loaderLayer .logo-anim .logo-dot {
  width: 6px;
  height: 6px;
  border-radius: 3px;
  background-color: #f2d31f;
  border: none;
  top: 23px;
  left: 25px;
}

.pageLoaderLayer {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  transform: translate(0,-100%);
  -webkit-transform: translate(0,-100%);
  z-index: 9999;
  display: none;
  font-size: 0;
  background-color: transparent;
  transition: transform 0.3s ease-out;
  -webkit-transition: transform 0.3s ease-out;
}

.pageLoaderLayer .full-loader {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10000;
  font-size: 0;
  background-color: #000;
}

.pageLoaderLayer .after {
  width: 100%;
  height: 50%;
  position: absolute;
  bottom: -50%;
  /* IE9 SVG, needs conditional override of 'filter' to 'none' */
  background: url(data:image/svg+xml;base64,);
  background: -moz-linear-gradient(top, rgba(0,0,0,1) 0%, rgba(0,0,0,0) 100%);
  /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,1)), color-stop(100%,rgba(0,0,0,0)));
  /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top, rgba(0,0,0,1) 0%,rgba(0,0,0,0) 100%);
  /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top, rgba(0,0,0,1) 0%,rgba(0,0,0,0) 100%);
  /* Opera 11.10+ */
  background: -ms-linear-gradient(top, rgba(0,0,0,1) 0%,rgba(0,0,0,0) 100%);
  /* IE10+ */
  background: linear-gradient(to bottom, rgba(0,0,0,1) 0%,rgba(0,0,0,0) 100%);
  /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#000000', endColorstr='#00000000',GradientType=0 );
  /* IE6-8 */
}

.pageLoaderLayer .before {
  width: 100%;
  height: 50%;
  position: absolute;
  top: -50%;
  /* IE9 SVG, needs conditional override of 'filter' to 'none' */
  background: url(data:image/svg+xml;base64,);
  background: -moz-linear-gradient(top, rgba(0,0,0,0) 0%, rgba(0,0,0,1) 100%);
  /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,0)), color-stop(100%,rgba(0,0,0,1)));
  /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top, rgba(0,0,0,0) 0%,rgba(0,0,0,1) 100%);
  /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top, rgba(0,0,0,0) 0%,rgba(0,0,0,1) 100%);
  /* Opera 11.10+ */
  background: -ms-linear-gradient(top, rgba(0,0,0,0) 0%,rgba(0,0,0,1) 100%);
  /* IE10+ */
  background: linear-gradient(to bottom, rgba(0,0,0,0) 0%,rgba(0,0,0,1) 100%);
  /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00000000', endColorstr='#000000',GradientType=0 );
  /* IE6-8 */
}

.pageLoaderLayer.active {
  transform: translate(0,0);
  -webkit-transform: translate(0,0);
}

.pageLoaderLayer.removed {
  transform: translate(0,100%);
  -webkit-transform: translate(0,100%);
}